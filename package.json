{
    "name": "tmsimple",
    "version": "1.0.0",
    "description": "Simple Test Case Management System",
    "scripts": {
        "build": "tsc --build",
        "eslint": "eslint . --ext .ts",
        "eslint:fix": "eslint . --ext .ts --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .   ",
        "start:server": "npm run -w @tmsimple/server start",
        "unit-test:server": "npm run -w @tmsimple/server unit-test",
        "component-test:server": "npm run -w @tmsimple/server component-test",
        "api-test": "npm --prefix ./test/e2e run api-test"
    },
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.13",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.2",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^22.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.5",
        "eslint-plugin-promise": "^6.0.1",
        "mocha": "^10.0.0",
        "prettier": "2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    },
    "author": "Daniel Jim√©nez Ortiz",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DanisaurioRex/tmsimple/issues"
    },
    "homepage": "https://github.com/DanisaurioRex/tmsimple#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DanisaurioRex/tmsimple.git"
    }
}
